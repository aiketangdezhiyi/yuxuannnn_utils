  "bugs": {
    "url": "https://github.com/aiketangdezhiyi/yuxuannnn_utils/issues"
  },
  "homepage": "https://github.com/aiketangdezhiyi/yuxuannnn_utils#readme",
  "devDependencies": {
    "@types/jest": "^29.2.3",
    "jest": "^29.3.1",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "dayjs": "^1.11.6"
  }
